{
  "exclude": ["node_modules/**/*"],
  "compilerOptions": {
    "module": "es2015",
    "declaration": true,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "jsx": "react",
    "lib": ["dom", "es7"],
    "target": "es5",
    "importHelpers": true,
    "outDir": "."
  }
}
